{
  "kind": "collectionType",
  "collectionName": "inquiries",
  "info": {
    "singularName": "inquiry",
    "pluralName": "inquiries",
    "displayName": "Inquiry",
    "description": "Inquiry form submissions from the homepage"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "preferred_lawn": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "desired_date": {
      "type": "date",
      "required": true
    },
    "guest_count": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "submitted_at": {
      "type": "datetime",
      "required": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "new",
        "contacted",
        "scheduled",
        "completed",
        "cancelled"
      ],
      "default": "new",
      "required": false
    },
    "notes": {
      "type": "text",
      "required": false
    }
  }
}
